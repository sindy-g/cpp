#include <iostream>
using namespace std;
int main{

int a = 0;
int b = 0;
int suma, resta, multiplicacion;
float division;

cout<<"Ingrese el valor de a: ";
cin>> a;
cout<<endl;

cout<<"Ingrese el valor de b: ";
cin>> b;
cout<<endl;

suma = a + b;
resta = a - b;
multiplicacion = a * b;
division = a / b;

cout<<"La suma es: " <<suma;
cout<<endl;
cout<<"La resta es: " <<resta;
cout<<endl;
cout<<"La multiplicacion es: " <<multiplicacion;
cout<<endl;
cout<<"La division es: " <<division;
cout<<endl;

    return 0;

}